{
  "name": "create-esboot",
  "version": "3.1.4",
  "description": "create-esboot",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "create-esboot": "bin/create-esboot.js"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "templates/mp/.gitignore",
    "templates/sp/.gitignore",
    "templates/demo/.gitignore"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "cross-env NODE_ENV=development tsup",
    "prepublishOnly": "pnpm run build",
    "sync": "bun run ./lib/sync/index.ts",
    "sync:all": "pnpm run sync mp && pnpm run sync sp && pnpm run sync demo",
    "test:sp": "mkdir -p ./tmp/esboot-sp && cd ./tmp/esboot-sp && node ../bin/create-esboot.js",
    "test:mp": "mkdir -p ../../tmp/esboot-mp && cd ../../tmp/esboot-mp && node ./bin/create-esboot.js"
  },
  "dependencies": {
    "@umijs/utils": "^4.0.75"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "chencheng <sorrycc@gmail.com> (https://github.com/sorrycc)",
    "roc <moonlitusun@gmail.com> (https://github.com/moonlitusun)"
  ]
}