{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": false,
    "noResolve": false,
    "removeComments": true,
    "baseUrl": ".",
    "paths": {
    }
  },
  "files": ["node_modules/.cache/esboot/typescript/esboot.d.ts"],
  "exclude": ["**/*.spec.ts"],
  "include": [".esbootrc.ts", "src/**/*", "globals.d.ts", "node_modules/.cache/esboot/typescript/esboot.d.ts"]
}
