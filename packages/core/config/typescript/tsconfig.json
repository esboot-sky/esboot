{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": false,
    "module": "ES2020",
    "moduleResolution": "node",
    "noResolve": false,
    "removeComments": true,
    "allowJs": false,
    "lib": [
      "ES5",
      "ES2015",
      "ES2018",
      "DOM"
    ],
    "target": "ES5"
  },
}
