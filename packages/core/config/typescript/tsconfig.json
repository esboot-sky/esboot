{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "nodeNext",
    "esModuleInterop": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": false,
    "noResolve": false,
    "removeComments": true
  },
}
