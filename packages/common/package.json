{
  "name": "@dz-web/esboot-common",
  "version": "3.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --minify --dts --format esm,cjs",
    "dev": "tsup --dts --watch --format esm,cjs",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": "./dist/constants/index.mjs",
      "default": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.mjs",
      "default": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "default": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers/index.mjs",
      "default": "./dist/helpers/index.js",
      "types": "./dist/helpers/index.d.ts"
    },
    "./radash": {
      "import": "./dist/third/radash/index.mjs",
      "default": "./dist/third/radash/index.js",
      "types": "./dist/third/radash/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "fs-extra": "^11.2.0",
    "kleur": "^4.1.5",
    "radash": "^12.1.0"
  },
  "tsup": {
    "entry": [
      "src/constants/index.ts",
      "src/helpers/index.ts",
      "src/utils/index.ts",
      "src/third/radash/index.ts"
    ],
    "clean": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}