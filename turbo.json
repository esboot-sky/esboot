{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "outputs": [
        "dist/**"
      ],
      "cache": true
    },
    "@dz-web/esboot-common#build": {},
    "@dz-web/esboot-lint#build": {
      "dependsOn": [
        "@dz-web/esboot-common#build"
      ]
    },
    "@dz-web/esboot#build": {
      "dependsOn": [
        "@dz-web/esboot-lint#build"
      ]
    },
    "@dz-web/esboot-bundler-common#build": {
      "dependsOn": [
        "@dz-web/esboot#build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    }
  }
}